    // panel nav
    {
        "key": "ctrl+w j",
        "command": "workbench.action.navigateDown"
    },
    {
        "key": "ctrl+w k",
        "command": "workbench.action.navigateUp"
    },
    {
        "key": "ctrl+w h",
        "command": "workbench.action.navigateLeft"
    },
    {
        "key": "ctrl+w l",
        "command": "workbench.action.navigateRight"
    },
    // sidebar control
    {
        "key": "ctrl+b",
        "command": "-vscode-neovim.send",
        "when": "editorTextFocus && neovim.ctrlKeysNormal && neovim.init && neovim.mode != 'insert'"
    },
    {
        "key": "ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+b b", // focus explorer
        "command": "workbench.files.action.focusFilesExplorer",
        "when": "!explorerViewletFocus"
    },
    {
        "key": "ctrl+b b", // exit sidebar
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "explorerViewletFocus"
    },
    {
        "key": "escape",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "sideBarFocus"
    },
    {
        "key": "ctrl+w c", // toggle sidebar
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+w f", // focus file in explorer
        "command": "workbench.files.action.showActiveFileInExplorer"
    },
    {
        "key": "ctrl+w r", // replace project
        "command": "workbench.action.replaceInFiles"
    },
    {
        "key": "ctrl+w g", // replace file
        "command": "editor.action.startFindReplaceAction"
    },
    {
        "key": "ctrl+w e", // extensions
        "command": "workbench.view.extensions"
    },
    {
        "key": "ctrl+w a", // activity bar toggle
        "command": "workbench.action.toggleActivityBarVisibility"
    },
    {
        "key": "ctrl+w s", // status bar toggle
        "command": "workbench.action.toggleStatusbarVisibility"
    },
    {
        "key": "ctrl+w o", // focus outline
        "command": "outline.focus"
    },
    {
        "key": "ctrl+n", // activity bar nav
        "command": "workbench.action.nextSideBarView",
        "when": "sideBarFocus"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.previousSideBarView",
        "when": "sideBarFocus"
    },
    {
        "key": "ctrl+b m",
        "command": "workbench.action.maximizeEditor",
    },
    {
        "key": "Escape",
        "command": "vscode-neovim.escape",
        "when": "editorTextFocus && neovim.mode == insert"
    }
]